{
  "name": "tudushka",
  "version": "1.0.0",
  "description": "Todo приложение с AI ассистентом",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail --restart-tries 3 \"npm run backend\" \"npm run frontend\"",
    "backend": "./venv/bin/python manage.py runserver",
    "frontend": "cd frontend && npm run dev",
    "install": "pip install -r requirements.txt && cd frontend && npm install",
    "test": "./venv/bin/python manage.py test"
  },
  "keywords": [
    "react",
    "django",
    "todo",
    "ai"
  ],
  "author": "Andrew T.",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
